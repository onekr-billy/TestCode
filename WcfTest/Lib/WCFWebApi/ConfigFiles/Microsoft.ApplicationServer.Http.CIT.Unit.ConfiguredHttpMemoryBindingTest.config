<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    
    <bindings>
      <httpMemoryBinding>
        <!-- named configured httpMemoryBinding -->
        <binding name="configuredBinding" />

        <!-- default configured httpMemoryBinding -->
        <binding />
