    </bindings>

    <extensions>
      <bindingExtensions>
        <add name="httpMemoryBinding" type="Microsoft.ApplicationServer.Http.Configuration.HttpMemoryBindingCollectionElement, Microsoft.ApplicationServer.Http"/>
      </bindingExtensions>
    </extensions>
    
  </system.serviceModel>
